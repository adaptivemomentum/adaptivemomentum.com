#head
  width: 100%
  height: 250px
  padding: 30px
  position: relative

#logo
  position: absolute
  left: -144px
  background: url('images/logo.png') center no-repeat
  width: 294px
  height: 197px

#links
  position: absolute
  top: 85px
  right: 0
  font-size: 200%

  .lsf
    margin: 0 10px
    display: inline-block
    position: relative
    color: #ddd
    border: 3px solid #ddd
    padding: 5px 10px
    height: 50px
    width: 50px
    border-radius: 50px
    transition: border-color .25s ease-in-out,
                color .25s ease-in-out,
                top .25s ease-in-out

  .lsf:hover
    color: #01B0F0
    border-color: #01B0F0
    top: -5px

  .rss
    padding-left: 13px

    &:last-child
      margin-right: 0
